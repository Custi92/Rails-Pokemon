<div class="container">
  <ul>
  <% @trainers.each do |trainer| %>
    <li>
      <%= link_to trainer.name, trainer_path(trainer) %>
      <%= trainer.age %>
      <%= cl_image_tag trainer.photo.key %>
    </li>
    <% end %>
  </ul>
  <%= simple_form_for(@trainers) do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :photo, as: :file %>
</div>
